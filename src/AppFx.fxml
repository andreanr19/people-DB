<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Label prefHeight="53.0" prefWidth="254.0" text="Ordenamiento Quick-Sort" BorderPane.alignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </top>
   <right>
      <Button mnemonicParsing="false" onAction="#agregarnumero" text="Agregar" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets right="50.0" />
         </BorderPane.margin>
      </Button>
   </right>
   <center>
      <TextField fx:id="intText" prefHeight="26.0" prefWidth="182.0" promptText="Ingrese numero entero" BorderPane.alignment="CENTER">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets left="100.0" right="100.0" />
         </BorderPane.margin>
      </TextField>
   </center>
   <bottom>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="arreglodislay" alignment="CENTER" contentDisplay="TOP" layoutX="6.0" layoutY="-5.0" prefHeight="84.0" prefWidth="589.0" text="Arreglo" textAlignment="CENTER" wrapText="true" />
            <Button layoutX="264.0" layoutY="87.0" mnemonicParsing="false" onAction="#ordenar" text="Ordenar" />
            <Label fx:id="tiempo" layoutX="442.0" layoutY="92.0" text="tiempo" />
            <Label fx:id="resultado" alignment="CENTER" layoutX="6.0" layoutY="128.0" prefHeight="53.0" prefWidth="585.0" wrapText="true" />
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
